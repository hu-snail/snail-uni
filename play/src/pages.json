{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "snail-uni-play",
    "navigationBarBackgroundColor": "#FFFFFF",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^su-(.*)": "@/components/su-$1.vue",
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue"
    }
  },
  "tabBar": {
    "color": "#99999",
    "selectedColor": "#5474f2",
    "backgroundColor": "#f5f5f5e6",
    "borderStyle": "white",
    "height": "50px",
    "fontSize": "12px",
    "iconWidth": "24px",
    "spacing": "3px",
    "blurEffect": "extralight",
    "list": [
      {
        "iconPath": "/static/tabbar/home_default.png",
        "selectedIconPath": "/static/tabbar/home_active.png",
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "iconPath": "/static/tabbar/temp_default.png",
        "selectedIconPath": "/static/tabbar/temp_active.png",
        "pagePath": "pages/template/index",
        "text": "模版"
      },
      {
        "iconPath": "/static/tabbar/my_default.png",
        "selectedIconPath": "/static/tabbar/my_active.png",
        "pagePath": "pages/my/index",
        "text": "我的"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "layout": "tabbar",
      "style": {
        "navigationBarTitleText": "首页",
        "backgroundColor": "#f5f5f5"
      },
      "name": "home"
    },
    {
      "path": "pages/my/index",
      "type": "page",
      "layout": "tabbar",
      "style": {
        "navigationBarTitleText": "我的"
      },
      "name": "my"
    },
    {
      "path": "pages/template/index",
      "type": "page",
      "layout": "tabbar",
      "style": {
        "navigationBarTitleText": "模版"
      },
      "name": "template"
    }
  ],
  "subPackages": [
    {
      "root": "sub-pages",
      "pages": [
        {
          "path": "axios/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "Axios 请求页",
            "backgroundColor": "#f5f5f5"
          }
        },
        {
          "path": "detail/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "详情页"
          }
        },
        {
          "path": "login/index",
          "type": "detail",
          "style": {
            "navigationBarTitleText": "登录"
          }
        },
        {
          "path": "route/index",
          "type": "page",
          "style": {
            "navigationBarTitleText": "Router 路由跳转",
            "backgroundColor": "#f5f5f5"
          }
        },
        {
          "path": "route/query",
          "type": "page",
          "style": {
            "navigationBarTitleText": "路由参数",
            "backgroundColor": "#f5f5f5"
          }
        }
      ]
    }
  ]
}
